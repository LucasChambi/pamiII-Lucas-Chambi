{
  "expo": {
    "name": "TitanHydra",
    "slug": "TitanHydra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://play-lh.googleusercontent.com/k2BaLq9MgfgJCSGSTi5HY26FcdiCwlkO5F9X6oZgBPrguMvnWjrWgsER_O8tlsLegZ0",
    "scheme": "titanhydra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://play-lh.googleusercontent.com/k2BaLq9MgfgJCSGSTi5HY26FcdiCwlkO5F9X6oZgBPrguMvnWjrWgsER_O8tlsLegZ0"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://play-lh.googleusercontent.com/k2BaLq9MgfgJCSGSTi5HY26FcdiCwlkO5F9X6oZgBPrguMvnWjrWgsER_O8tlsLegZ0",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
